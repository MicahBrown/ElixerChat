<%= form_for @changeset, @action, [id: "chat-form"], fn _f -> %>
  <%= if false && @changeset.action do %>
    <div class="alert alert-danger">
      <p>Oops, something went wrong! Please check the errors below.</p>
    </div>
  <% end %>

  <div class="g-recaptcha" data-sitekey="<%= Recaptcha.key %>" data-callback="submitChatForm"></div>
<% end %>
